{
    "compilerOptions": {
      "target": "es6",
      "module": "es6",
      "moduleResolution": "node",
      "declaration": false,
      "noImplicitAny": false,
      "sourceMap": true,
      "noLib": false,
      "suppressImplicitAnyIndexErrors": true,
      "strict": true,
      "paths": {
        "@/*": [
          "src/*"
        ]
      }
    },
    "compileOnSave": false,
    "exclude": [
      "node_modules"
    ]
  }

{
    "compilerOptions": {
      "target": "es6",
      "module": "es6",
      "moduleResolution": "node",
      "declaration": false,
      "noImplicitAny": false,
      "sourceMap": true,
      "noLib": false,
      "suppressImplicitAnyIndexErrors": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "lib": [
        "dom",
        "es5",
        "es2015.promise"
      ]
    },
    "compileOnSave": false,
    "exclude": [
      "node_modules"
    ]
  }
